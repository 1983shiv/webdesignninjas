{
  "name": "webdesign-ninjas",
  "description": "Webdesign Ninjas - We Transform your digital Idea's into Reality",
  "version": "0.0.1",
  "scripts": {
    "dev2": "sapper dev && postcss static/tailwind.css -o static/index.css -w",
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "build2": "sapper build --legacy && npm run export && node ./scripts/gh-pages.js",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "lint": "eslint . --ext .js,.svelte",
    "tailwind-build": "tailwindcss build static/tailwind.css -o static/index.css",
    "tailwindpostcss": "postcss static/tailwind.css -o static/index.css"
  },
  "dependencies": {
    "autoprefixer": "^9",
    "compression": "^1.7.1",
    "polka": "next",
    "postcss": "^8.3.0",
    "prismjs": "^1.23.0",
    "sirv": "^0.4.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@beyonk/svelte-carousel": "^2.8.0",
    "@jackfranklin/rollup-plugin-markdown": "^0.2.0",
    "@rollup/plugin-alias": "^3.0.1",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@rollup/plugin-url": "^5.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-svelte3": "^2.7.3",
    "gh-pages": "^3.2.0",
    "lodash": "^4.17.19",
    "moment": "^2.29.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1",
    "prism-themes": "^1.5.0",
    "rollup": "^2.6.1",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-glob": "^1.0.2",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^5.3.0",
    "sapper": "^0.28.0",
